<tr class="{% if tab.spec %}special{% else %}{{ style }}{% endif %}" id="rw{{ tabid }}">
      <td>
	<input name="id" type="text" value={{ tabid }} readonly size="3" style="Text-align:right">
	<a id="{{ tabid }}"></a></td>
      <td>
	<input name="date" type="text" id="dat{{ tabid }}" value="{{ tab.date }}" size=8 {% if tabid!='new' %}onChange="javascript:ajax(rw{{ tabid }});"{% endif %} ondblclick="javascript:NewCssCal('dat{{ tabid }}','ddMMyyyy', 'arrow');" autocomplete="off"></td>
	<td id="x{{ tabid }}">
	  {% include
	  'dodropbox.html.twig' with
	  {'na':'cp_s','z0':false,'sele':tab.cpS==NULL?-1:tab.cpS,'add':'style="width:70px"','add2':tabid,'res':comptes
	  }%}
	</td>
	<td>
	  {% include
	  'dodropbox.html.twig' with
	  {'na':'cp_d','z0':'--------','sele':tab.cpD==NULL?-1:tab.cpD,'add':'','add2':tabid,'res':comptes
	  }%}
	</td>
	<td>
	  {% include
	  'dodropbox.html.twig' with
	  {'na':'moy','z0':'----','sele':tab.moyen==NULL?-1:tab.moyen,'add':'', 'add2':tabid,'res':moyens
	  }%}
	</td>
	<td>
	  {% include
    'dodropbox.html.twig' with
	  {'na':'cat','z0':'--------','sele':tab.category==NULL?0:tab.category,'add':'','add2':tabid,'res':categories
	  }%}
	</td>
	<td>
	  <input name="com" type="text" value="{{ tab.com }}" {% if tabid!='new' %}onChange="javascript:ajax(rw{{ tabid }});"{% endif %}></td>
	  <td>
	    <input id="pr{{ tabid }}" name="pr" type="text" value="{{ tab.pr/100 }}" size=7 style="Text-Align:right;color:{% if tab.pr>0 %}#00AA00{% else %}#AA0000{% endif %}" onChange="javascript:ajax(rw{{ tabid }});" ondblclick="javascript:change_signe({{ tabid }});javascript:ajax(rw{{ tabid }});" autocomplete="off"></td>
	  <td>
	    <input id="pt{{ tabid }}" name="pt" class="check" type="checkbox" onChange="javascript:ajax(rw{{ tabid }});" {% if tab.poS==1 %}checked{% endif %}></td>
	  <td style="Text-Align:right" id="so{{ tabid }}">{{ soldes[tabid] / 100 }}</td>
	  <td>
	  {% if tabid!='new' %}
	    <button onClick="javascript:duplicate(rw{{ tabid }}, rwnew);" name="dupl">
	  {% else %}
	    <button onClick="javascript:ajax(rw{{ tabid }});" name="add">
	  {% endif %}
	      <img src="/bundles/clemlafcomptesapp/images/b_snewtbl.png"></button></td>
	  <td>
	      <button onClick={% if tabid!='new' %}"javascript:supprime(rw{{ tabid }});"{% else %}"javascript:clearline(rw{{ tabid }});"{% endif %} name="supp" type="reset">
	      <img src="/bundles/clemlafcomptesapp/images/b_drop.png"></button></td>
    </tr>    
