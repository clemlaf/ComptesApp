<?xml version="1.0" encoding="UTF-8"?>
<table>
<type>
{{ type }}
</type>
{% for a in entries %}
  <entry>
    <id>{{ a['id'] }}</id>
    <solde>{{ a['solde'] }}</solde>
  </entry>
{% endfor %}
<soldepointe>{{ soldepointe }}</soldepointe>
<soldefiltre>{{ soldefiltre }}</soldefiltre>
<newentry>
  <id>{{ tab.id }}</id>
  <date>{{ tab.date|date('d/m/Y') }}</date>
  <cp_s>{{ tab.cpS==null?null:tab.cpS.id }}</cp_s>
  <cp_d>{{ tab.cpD==null?null:tab.cpD.id }}</cp_d>
  <cat>{{ tab.category==null?null:tab.category.id }}</cat>
  <moy>{{ tab.moyen==null?null:tab.moyen.id }}</moy>
  <com>{{ tab.com }}</com>
  <pr>{{ tab.pr/100 }}</pr>
  <pt>{{ tab.poS }}</pt>
</newentry>
</table>
